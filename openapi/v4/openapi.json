    "/v4/assets": {
      "get": {
        "tags": [
          "Assets"
        ],
        "summary": "Provides a list of assets in GeoJSON format",
        "description": "This operation allows for discovery of assets. It returns a list of assets matching the search parameters and the centroid of the assets location. The asset's actual geometry type is made available in the response and the actual geometry can be retrieved at the asset detail endpoint",
        "operationId": "get-assets",
        "parameters": [
          {
            "$ref": "#/components/parameters/limit"
          },
          {
            "$ref": "#/components/parameters/offset"
          },
          {
            "$ref": "#/components/parameters/countries_csv"
          },
          {
            "$ref": "#/components/parameters/sectors_csv"
          },
          {
            "$ref": "#/components/parameters/gas"
          },
          {
            "$ref": "#/components/parameters/continents_csv_optional"
          }
        ],
        "responses": {
          "200": {
            "description": "List of Assets",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "https://geojson.org/schema/FeatureCollection.json"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/400"
          }
        }
      }
    },
    "/v4/assets/{assetId}": {
      "get": {
        "tags": [
          "Assets"
        ],
        "summary": "Provides detail about a single asset, including ownership information",
        "description": "Provides detail about a single asset, including ownership information",
        "operationId": "get-asset-by-id",
        "parameters": [
          {
            "name": "assetId",
            "description": "The id of the asset",
            "in": "path",
            "required": true,
            "allowEmptyValue": false,
            "schema": {
              "type": "integer",
              "minimum": 1,
              "maximum": 5000000
            }
          }
        ],
        "responses": {
          "200": {
            "description": "List of Assets",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "https://geojson.org/schema/FeatureCollection.json"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/400"
          }
        }
      }
    },
    "/v4/assets/countries": {
      "get": {
        "tags": [
          "Assets",
          "Summaries"
        ],
        "summary": "Provides a list of countries (ISO3 codes) and the number of assets within it",
        "description": "",
        "operationId": "get-asset-count-by-country",
        "responses": {
          "200": {
            "description": "List of country codes and the number of assets in each",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "additionalProperties": {
                    "type": "integer"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/v4/assets/emissions/remainders": {
      "get": {
        "tags": [
          "Emissions"
        ],
        "summary": "Provides a list of remainders by sector",
        "description": "",
        "operationId": "get-remainders",
        "parameters": [
          {
            "$ref": "#/components/parameters/countries_csv"
          },
          {
            "$ref": "#/components/parameters/years_csv"
          }
        ],
        "responses": {
          "200": {
            "description": "List of emission remainders for the parameters provided",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "Country": {
                        "type": "string",
                        "maxLength": 3,
                        "description": "Country Code"
                      },
                      "Year": {
                        "type": "integer"
                      },
                      "Gas": {
                        "$ref": "#/components/schemas/Gas"
                      },
                      "Sector": {
                        "type": "string"
                      }
                    }
                  }
                }
              }
            }
          },
          "400": {

            "$ref": "#/components/responses/400"
          }
        }
      }
    },

    "/v4/country/emissions/timeseries/sectors": {
      "get": {
        "tags": [
          "Emissions",
          "Time Series"
        ],
        "summary": "Provides emissions time-series emissions data, broken down by sector",
        "description": "This operation replaces /emissions_by_sector_time-series in the v1 API",
        "operationId": "get-emissions-timeseries-sector",
        "parameters": [
          {"$ref": "#/components/parameters/countries_csv_optional"},
          {
              "$ref": "#/components/parameters/since"
            },
          {"$ref": "#/components/parameters/to"}

        ],
        "responses": {
          "200": {
            "description": "A time-series of emissions grouped by sector",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/TimeSeriesEmission"
                }
              }
            }
          },
          "400": {
            "description": "An invalid request was received",
            "content": {
              "application/problem+json": {
                "schema": {
                  "$ref": "#/components/schemas/Problem"
                }
              }
            }
          },
          "404": {
            "description": "The requested item was not found",
            "content": {
              "application/problem+json": {
                "schema": {
                  "$ref": "#/components/schemas/Problem"
                }
              }
            }
          }
        }
      }
    },
    "/v4/country/emissions/timeseries/sectors/{sector}": {
      "get": {
        "tags": [
          "Emissions",
          "Time Series"
        ],
        "summary": "Provides emissions time-series detail by subsector",
        "description": "This operation replaces /emissions_by_subsector_timeseries in the v1 API. One of the country or continent parameters is required - continent takes precedence if both are provided ",
        "operationId": "get-emissions-timeseries-subsector",
        "parameters": [
          {
            "name": "countries",
            "in": "query",
            "description": "A comma separated list of 3 letter code specifying the countries",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "continents",
            "in": "query",
            "description": "A comma separated list of continents",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "enum": ["Africa",
                "Europe",
                "South America",
                "Oceania",
                "Antarctica",
                "North America",
                "Asia"],
              "type": "string"
            }
          },
          {
            "name": "since",
            "in": "query",
            "description": "The start year (inclusive). Must be less than 'to' parameter",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "maximum": 2050,
              "minimum": 2000,
              "type": "integer",
              "default": 2010
            }
          },
          {
            "name": "to",
            "in": "query",
            "description": "The end year (inclusive). Must be larger than 'since' parameter",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "maximum": 2050,
              "minimum": 2000,
              "type": "integer",
              "default": 2020
            }
          },
          {
            "name": "sector",
            "in": "path",
            "description": "The Sector",
            "required": true,
            "style": "simple",
            "explode": false,
            "schema": {
              "enum": ["fossil-fuel-operations",
                "power",
                "waste",
                "agriculture",
                "mineral-extraction",
                "fluorinated-gases",
                "forestry-and-land-use",
                "manufacturing",
                "buildings",
                "transportation"],
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "A time-series of emissions grouped by SubSectors",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/TimeSeriesEmission"
                }
              }
            }
          },
          "400": {
            "description": "An invalid request was received",
            "content": {
              "application/problem+json": {
                "schema": {
                  "$ref": "#/components/schemas/Problem"
                }
              }
            }
          },
          "404": {
            "description": "The requested item was not found",
            "content": {
              "application/problem+json": {
                "schema": {
                  "$ref": "#/components/schemas/Problem"
                }
              }
            }
          }
        }
      }
    },
